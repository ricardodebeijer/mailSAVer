<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
  </ribbon>
  <contextMenus>
    <contextMenu idMso="ContextMenuMailItem">
      <menu id="MailSAVermenu" label="MailSAVer" insertAfterMso="Copy" getImage="GetIcon">
        <button id="btnSave"
            label="Sla Op"
            imageMso="FileSave"
            onAction="btnSaveClick"/>
        <button id="btnSaveOther"
           label="Sla Op Anders"
           imageMso="FileSaveAs"
           onAction="btnSaveOtherClick"/>
        <button id="btnNewProject"
          label="Nieuw Project + Sla Op"
          imageMso="MoveToFolder"
          onAction="btnNewProjectClick"/>
        <button id="btnSettings"
          label="Instellingen"
          imageMso="FileServerTasksMenu"
          onAction="btnSetttingsClick"/>
      </menu>
    </contextMenu>
  </contextMenus>
</customUI>