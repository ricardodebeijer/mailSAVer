<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <contextMenus>
    <contextMenu idMso="ContextMenuMailItem">
      <menu id="MailSAVermenu" label="MailSAVer" insertAfterMso="Copy" getImage="GetGroupImage">
        <button id="btnSlaOp"
            label="Sla Op" 
            imageMso="FileSave"
            onAction="btnSlaOpClick"/>
        <button id="btnSlaOpAnders"
           label="Sla Op Anders"
           imageMso="FileSaveAs"
           onAction="btnSlaOpAndersClick"/>
        <button id="btnNiewProject"
          label="Nieuw Project + Sla Op"
          imageMso="MoveToFolder" 
          onAction="btnNieuwProjectClick"/>
        <button id="btnInstellingen"
          label="Instellingen"
          imageMso="FileServerTasksMenu"
          onAction="btnInstellingenClick"/>
      </menu>
    </contextMenu>
  </contextMenus>
</customUI>

